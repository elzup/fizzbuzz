{
  "name": "@elzup/fizzbuzz",
  "version": "0.0.0",
  "description": "elzup&#39;s rich fizz buzz",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "elzup/fizzbuzz",
  "author": "elzup <guild0105@gmail.com> (elzup.com)",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "babel src --out-dir dist && npm run prepack:flow",
    "deploy": "npm run build && yarn publish",
    "prepack:flow": "flow-copy-source src dist",
    "add-contributor": "all-contributors add",
    "precommit": "yarn flow && lint-staged",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest src",
    "test:cov": "jest src --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    ""
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-flow": "^7.0.0-beta.54",
    "all-contributors-cli": "^5.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "eslint": "^5.1.0",
    "eslint-config-precure": "^2.7.0",
    "flow-copy-source": "^2.0.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0"
  }
}

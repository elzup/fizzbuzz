{
  "name": "@elzup/fizzbuzz",
  "version": "0.0.2",
  "description": "elzup&#39;s rich fizz buzz",
  "author": "elzup <guild0105@gmail.com> (elzup.com)",
  "access": "public",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "elzup/fizzbuzz",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "babel src --out-dir dist && npm run prepack:flow",
    "deploy": "npm run build && yarn publish",
    "prepack:flow": "flow-copy-source src dist",
    "add-contributor": "all-contributors add",
    "precommit": "yarn flow && lint-staged",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest src",
    "test:cov": "jest src --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    ""
  ],
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-flow": "7.0.0",
    "all-contributors-cli": "6.9.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "eslint-config-precure": "5.1.0",
    "flow-copy-source": "2.0.8",
    "husky": "0.14.3",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2"
  }
}
